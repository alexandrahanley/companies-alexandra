<h1>All Industries</h1>

<%= link_to 'Create New Industry', new_industry_path %>

<table>
  <tr>
      <th>Name</th>
      <th>Created At</th>
      <th>Updated At</th>
  </tr>

    <tbody>
      <%= @industries.each do |industry| %>
        <tr>
          <td><%= link_to industry.name, industry_path(@industry) %></td>
          <td><%=industry.created_at %></td>
          <td><%=industry.updated_at %></td>
       </tr>
          <% end %>
    </tbody>
</table>
